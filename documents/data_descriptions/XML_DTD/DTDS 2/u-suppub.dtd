<!ELEMENT supplemental-pub-data  (supp.title-block,supp.data-block) >

<!ATTLIST supplemental-pub-data
	       id  ID    #REQUIRED 
	       label  CDATA    #FIXED "[Supplemental Publication Data:] "
	       
	       class  CDATA    #FIXED "legal-instrument" >

<!ELEMENT supp.title-block  (supp.title,supp.version) >

<!ATTLIST supp.title-block
	       label  CDATA    #FIXED "[Title Block:] "
	       class  CDATA    #FIXED "template-id" >

<!ELEMENT supp.title   EMPTY  >

<!ATTLIST supp.title
	       label  CDATA    #FIXED "[Title:] "
	       class  CDATA    #FIXED "title"
	       text  CDATA    #FIXED "SUPPLEMENTAL PUBLICATION DATA"
		>

<!ELEMENT supp.version   (#PCDATA)  >

<!ATTLIST supp.version
	       class  CDATA    #FIXED "version"
	       distribution  CDATA    #FIXED "2000-09-27"
	       label  CDATA    #FIXED "[Version:] "
	       version  CDATA    #FIXED "[Electronic Version 0.0.5 "
		>

<!ELEMENT supp.data-block  (application-number,domestic-filing-data,
	       projected-publication-date,representative-figure?,classification-us,
	       classification-ipc,prior-publication?,international-conventions?,
	       sequence-listing-available?) >

<!ATTLIST supp.data-block
	       label  CDATA    #FIXED "[Supplemental Data Block:] "
		>

