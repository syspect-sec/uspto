<!ELEMENT specification  (spec.title-block,specification-block) >

<!ATTLIST specification
	       id  ID    #REQUIRED 
	       label  CDATA    #FIXED "[Specification:] "
	       class  CDATA    #FIXED "legal-instrument" >

<!-- Unknown symbol to be placed in PCDATA fields when unknown symbol 
     is placed in text by specification author. -->
<!ELEMENT unknown-symbol EMPTY>

<!ELEMENT spec.title-block  (spec.title,spec.version) >

<!ATTLIST spec.title-block
	       label  CDATA    #FIXED "[Title Block:] "
	       class  CDATA    #FIXED "template-id" >

<!ELEMENT specification-block  (title-of-invention,
	       cross-reference-to-related-applications?,copyright-statement?,
	       federal-research-statement?,appendix-data?,background-of-invention?,
	       summary-of-invention?,brief-description-of-drawings?,
	       brief-description-of-sequences?,detailed-description,
	       program-listing-deposit?,claims,abstract-of-disclosure,figures?) >

<!ATTLIST specification-block
	       label  CDATA    #FIXED "[Specification Block:] " >

<!ELEMENT spec.title   EMPTY  >

<!ATTLIST spec.title
	       label  CDATA    #FIXED "[Title:] "
	       class  CDATA    #FIXED "title"
	       text  CDATA    #FIXED "SPECIFICATION" >

<!ELEMENT spec.version   EMPTY  >

<!ATTLIST spec.version
	       label  CDATA    #FIXED "[Version:] "
	       class  CDATA    #FIXED "version"
	       version  CDATA    #FIXED "[Electronic Version 1.2.8] "
	       
	       distribution  CDATA    #FIXED "2000-10-16" >

<!ELEMENT title-of-invention  (#PCDATA | emphasis | superscript | subscript)* >

<!ATTLIST title-of-invention
	       id  ID    #REQUIRED 
	       class  CDATA    #FIXED "title"
	       label  CDATA    #FIXED "[Title of Invention:] " >

